<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">
    <include
        layout="@layout/layout_action_bar"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <include
                layout="@layout/layout_head_upload"/>
            <LinearLayout
                android:id="@+id/layout_up"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"></LinearLayout>
            <com.example.user.live.view.StaticListView
                android:id="@+id/lv_finish"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></com.example.user.live.view.StaticListView>
            <com.example.user.live.view.StaticListView
                android:id="@+id/lv_fail"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></com.example.user.live.view.StaticListView>
        </LinearLayout>
    </ScrollView>
</LinearLayout>